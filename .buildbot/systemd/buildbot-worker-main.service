[Unit]
Description=BuildBot main worker service
After=network.target

[Service]
User=buildbot
Group=buildbot
WorkingDirectory=/home/buildbot/bb-lms/worker_for_main
ExecStart=/home/buildbot/bb-lms/venv/bin/buildbot-worker start --nodaemon
Environment="PATH=/home/buildbot/bb-lms/venv/bin"


[Install]
WantedBy=buildbot-master.target

